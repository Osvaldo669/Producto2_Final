<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppMovil.Views.Menu.MenuFlyoutPage"
             xmlns:pages="clr-namespace:AppMovil.Views.Menu"
            xmlns:inicio ="clr-namespace:AppMovil"
            NavigationPage.HasBackButton="False"
            NavigationPage.HasNavigationBar="False"
            >
  <FlyoutPage.Flyout>
    <pages:MenuFlyoutPageFlyout x:Name="FlyoutPage" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <inicio:MainPage />
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>